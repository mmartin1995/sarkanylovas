{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/about",
        "dest": "/about.html"
      }
    ],
    "env": {
      "CUSTOM_ENV_VARIABLE": "@my_custom_value"
    },
    "build": {
      "env": {
        "BUILD_ENV_VARIABLE": "@my_build_value"
      }
    }
  }
  